@using Yatzy.Models

<div class="dicecontainer">
	@foreach (var die in dice)
	{

		@if (die.currentRoll == 1)
		{
			<div class="m-3 face one">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>

			</div>
		}
		else if (die.currentRoll == 2)
		{
			<div class="m-3 face two">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>
			</div>
		}
		else if (die.currentRoll == 3)
		{
			<div class="m-3 face three">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>
			</div>
		}
		else if (die.currentRoll == 4)
		{
			<div class="m-3 face four">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>
			</div>
		}
		else if (die.currentRoll == 5)
		{
			<div class="m-3 face five">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>
			</div>
		}
		else if (die.currentRoll == 6)
		{
			<div class="m-3 face six">
				<button class="p-0 img-button-dice" @onclick="(() =>SaveDice(die))"><img class="img-button-dice" src="data:image/png;base64," width="50" /></button>
				<p class="text-center">@die.SaveRoll</p>
			</div>
		}
		else if(ResetDice)
		{
			<p>Press the button to roll the dice</p>
			break;
		}
		else
		{
			<p>Press the button to roll the dice</p>
			break;
		}
	}
</div>
<div class="d-flex justify-content-center m-0">
	<button class="btn btn-dark p-4" @onclick="RollDice">Roll</button>
</div>
@code {
	List<int> SavedDiceList = SavedDice.SavedDiceList;
	public List<Dice> dice = new List<Dice>()
{
	new Dice(),
	new Dice(),
	new Dice(),
	new Dice(),
	new Dice(),
};

	[Parameter]
	public EventCallback PlayerThrows { get; set; }


	[Parameter]
	public EventCallback AddToScoreBoardList { get; set; }

	[Parameter]
	public int throws { get; set; }

	[Parameter]
	public bool ResetDice { get; set; }



	private void RollDice()
	{
		for (int i = 0; i < dice.Count; i++)
			if (dice[i].currentRollBool == false && throws > 1)
			{
				dice[i].RollDice();
				
			}
			else if(throws == 1)
			{

				if(dice[i].currentRollBool == false)
				{
					dice[i].RollDice();
					SavedDiceList.Add(dice[i].currentRoll);
					dice[i].currentRollBool = true;
					dice[i].SaveRoll = "Saved";
				}

			}
		AddToScoreBoardList.InvokeAsync();
		PlayerThrows.InvokeAsync();
	}
	private void SaveDice(Dice die)
	{
		if (!die.currentRollBool)
		{
			SavedDiceList.Add(die.currentRoll);
			die.currentRollBool = true;
			die.SaveRoll = "Saved";
			AddToScoreBoardList.InvokeAsync();
		}
		else if (die.currentRollBool)
		{
			SavedDiceList.Remove(die.currentRoll);
			die.currentRollBool = false;
			die.SaveRoll = "";
			AddToScoreBoardList.InvokeAsync();
		}

	}
}
